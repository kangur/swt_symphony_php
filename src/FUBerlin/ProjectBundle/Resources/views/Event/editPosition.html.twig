{% extends 'FUBerlinProjectBundle::base.html.twig' %}

{% block content %}

<form class="form-horizontal well" action="{{ path('position_edit', {'id' :position.id}) }}" method="post" {{ form_enctype(form) }}>
    <fieldset>
        <legend>{{'Edit Position'|trans}}</legend>
    
        {{ form_widget(form) }}

        <div class="form-actions">
            <button class="btn btn-primary" type="submit">{{'Save'|trans}}</button>
            <button class="btn" type="reset">{{'Clear'|trans}}</button>
        </div>
        
    </fieldset>
</form>
{% endblock %}